{
  "name": "parfumo-extract",
  "version": "1.0.0",
  "description": "Web crawler for extracting fragrance data from Parfumo.com",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "crawl": "tsx src/crawler/orchestrator.ts",
    "db:migrate": "tsx src/database/migrate.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "p-limit": "^5.0.0",
    "p-retry": "^6.2.0",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "robotparser": "^2.3.0",
    "user-agents": "^1.1.322"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/user-agents": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}